{
  "version": 1,
  "build": {
    "packtool": ""
  },
  "canisters": {
    "arbitra_backend": {
      "main": "src/arbitra_backend/main.mo",
      "type": "motoko",
      "source": [
        "src/arbitra_backend"
      ]
    },
    "evidence_manager": {
      "main": "src/evidence_manager/main.mo",
      "type": "motoko"
    },
    "ai_analysis": {
      "main": "src/ai_analysis/main.mo",
      "type": "motoko"
    },
    "bitcoin_escrow": {
      "main": "src/bitcoin_escrow/main.mo",
      "type": "motoko"
    },
    "legal_framework": {
      "main": "src/legal_framework/main.mo",
      "type": "motoko"
    },
    "legal_agreements": {
      "main": "src/legal_agreements/main.mo",
      "type": "motoko"
    },
    "legal_compliance": {
      "main": "src/legal_compliance/main.mo",
      "type": "motoko"
    },
    "arbitrator_registry": {
      "main": "src/arbitrator_registry/main.mo",
      "type": "motoko"
    },
    "evidence_chain": {
      "main": "src/evidence_chain/main.mo",
      "type": "motoko"
    },
    "evidence_verifier": {
      "main": "src/evidence_verifier/main.mo",
      "type": "motoko"
    },
    "arbitra_frontend": {
      "dependencies": [
        "arbitra_backend",
        "evidence_manager",
        "ai_analysis",
        "bitcoin_escrow",
        "legal_framework",
        "legal_agreements",
        "legal_compliance",
        "arbitrator_registry",
        "evidence_chain",
        "evidence_verifier"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    }
  },
  "output_env_file": ".env.local"
}
